#creating dataframe for standard deviation and renaming rows "STDEV"
st.dev <- tapply(X = fish$parcel.start.lat,INDEX = fish$tow.depth,FUN = sd)
data.frame(st.dev)
stdev.p <- data.frame(st.dev)
stdev.p$STDEV <- rownames(stdev.p)
stdev.p
#creating dataframe for mean and renaming rows "MEAN
mean <-tapply(X = fish$parcel.start.lat,INDEX = fish$tow.depth,FUN = mean)
data.frame(mean)
mean.p <- data.frame(mean)
mean.p$MEAN<- rownames(mean.p)
mean.p
#merging by STDEV and MEAN
mean.stdev <- merge(x = stdev.p,y = mean.p,by.x = "STDEV",by.y = "MEAN")
mean.stdev
